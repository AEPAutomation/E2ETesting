	String ret = &quot;&quot;;

	while(true) {
		int ci = (int)(Math.random()*123);
		if((ci &lt;= 57 &amp;&amp; ci &gt;= 48) || (ci &lt;= 90 &amp;&amp; ci &gt;= 65) || (ci &lt;= 122 &amp;&amp; ci &gt;= 97)) {
			char ch = (char) ci;
			ret = ret + ch;
